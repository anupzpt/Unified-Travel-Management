@model HotelApp.Shared.Home.HomeCommon
@{
    ViewData["Title"] = "Home Page";
}
<!-- Carousel Start -->
<div class="container-fluid position-relative p-0">
    <nav class="navbar navbar-expand-lg navbar-light px-4 px-lg-5 py-3 py-lg-0">
        <a href="" class="navbar-brand p-0">
            <h1 class="m-0"><img src="~/admin/assets/img/layouts/logo2_2cc3.png" alt="IMG" style="width: 80px; height: 80px; border-radius: 50%; background-color:transparent"> Travela</h1>
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
            <span class="fa fa-bars"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto py-0">
                <a href="@Url.Action("Index","Home")" class="nav-item nav-link">Home</a>
                <a href="@Url.Action("Package","Home")" class="nav-item nav-link">Packages</a>
                <a href="@Url.Action("Accomodation","Home")" class="nav-item nav-link">Accomodation</a>
                <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Pages</a>
                    <div class="dropdown-menu m-0">
                        <a href="@Url.Action("Gallery","Home")" class="dropdown-item">Our Gallery</a>
                        <a href="@Url.Action("Guide","Home")" class="dropdown-item">Travel Guides</a>
                        <a href="@Url.Action("Blog","Home")" class="dropdown-item">Blog</a>
                    </div>
                </div>
                <a href="@Url.Action("Contact","Home")" class="nav-item nav-link">Contact</a>
            </div>
            <a href="@Url.Action("PlanATrip","Home")" class="btn btn-primary rounded-pill py-2 px-4 ms-lg-4">Plan A Trip</a>
        </div>
    </nav>

    <div class="carousel-header">
        <div id="carouselId" class="carousel slide" data-bs-ride="carousel">
            <ol class="carousel-indicators">
                <li data-bs-target="#carouselId" data-bs-slide-to="0" class="active"></li>
                <li data-bs-target="#carouselId" data-bs-slide-to="1"></li>
                <li data-bs-target="#carouselId" data-bs-slide-to="2"></li>
            </ol>
            <div class="carousel-inner" role="listbox">
                @if (Model.BannerDetailsList.Count() > 0)
                {
                    for (int i = 0; i <= 0; i++)
                    {
                        <div class="carousel-item active">
                            <img src="~/ProjectImage/@Model.BannerDetailsList[i].BannerImageView" class="img-fluid" alt="Image">
                            <div class="carousel-caption">
                                <div class="p-3" style="max-width: 900px;">
                                    <h4 class="text-white text-uppercase fw-bold mb-4" style="letter-spacing: 3px;">@Model.BannerDetailsList[i].Title</h4>
                                    <h1 class="display-2 text-capitalize text-white mb-4">@Model.BannerDetailsList[i].SloganName</h1>
                                    <p class="mb-5 fs-5">
                                        @Model.BannerDetailsList[i].Description
                                    </p>
                                    <div class="d-flex align-items-center justify-content-center">
                                        <a class="btn-hover-bg btn btn-primary rounded-pill text-white py-3 px-5" href="@Url.Action("Accomodation","Home")">Discover Now</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                    for (int i = 1; i < Model.BannerDetailsList.Count(); i++)
                    {
                        <div class="carousel-item">
                            <img src="~/ProjectImage/@Model.BannerDetailsList[i].BannerImageView" class="img-fluid" alt="Image">
                            <div class="carousel-caption">
                                <div class="p-3" style="max-width: 900px;">
                                    <h4 class="text-white text-uppercase fw-bold mb-4" style="letter-spacing: 3px;">@Model.BannerDetailsList[i].Title</h4>
                                    <h1 class="display-2 text-capitalize text-white mb-4">@Model.BannerDetailsList[i].SloganName</h1>
                                    <p class="mb-5 fs-5">
                                        @Model.BannerDetailsList[i].Description
                                    </p>
                                    <div class="d-flex align-items-center justify-content-center">
                                        <a class="btn-hover-bg btn btn-primary rounded-pill text-white py-3 px-5" href="#">Discover Now</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                }
                else
                {
                    <div class="carousel-item active">
                        <img src="~/admin/assets/img/image/pexels-karolina-2031292-3669288.jpg" class="img-fluid" alt="Image">
                        <div class="carousel-caption">
                            <div class="p-3" style="max-width: 900px;">
                                <h4 class="text-white text-uppercase fw-bold mb-4" style="letter-spacing: 3px;">Explore The World</h4>
                                <h1 class="display-2 text-capitalize text-white mb-4">Let's The World Together!</h1>
                                <p class="mb-5 fs-5">
                                    Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s,
                                </p>
                                <div class="d-flex align-items-center justify-content-center">
                                    <a class="btn-hover-bg btn btn-primary rounded-pill text-white py-3 px-5" href="@Url.Action("HotelList","Accomodation")">Discover Now</a>
                                </div>
                            </div>
                        </div>
                    </div>
                }

            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselId" data-bs-slide="prev">
                <span class="carousel-control-prev-icon btn bg-primary" aria-hidden="false"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselId" data-bs-slide="next">
                <span class="carousel-control-next-icon btn bg-primary" aria-hidden="false"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>
    <!-- Carousel End -->
</div>

