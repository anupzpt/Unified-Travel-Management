@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers;
@model HotelApp.Shared.Login.LoginCommon;
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Unified Travel Experience System</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/user/css/style.css" rel="stylesheet" />
    <link href="~/user/css/bootstrap.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link rel="icon" href="~/admin/assets/img/layouts/logo2_2cc3.png" type="image/png">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/login/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/login/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/login/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/login/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/login/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/login/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/login/css/main.css">
    <!--===============================================================================================-->
</head>
<div class="container-fluid  position-relative p-0 " style="background-color:#13357b">
    <nav class="navbar navbar-expand-lg navbar-dark  px-4 px-lg-5 py-3 py-lg-0">
        <a href="" class="navbar-brand p-0">
            <h1 class="m-0" style="color:whitesmoke; margin-right: 20px;"><img src="~/admin/assets/img/layouts/logo2_2cc3.png" alt="IMG" style="width: 80px; height: 80px; border-radius: 50%;"> Travela</h1>
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
            <span class="fa fa-bars"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto py-0">
                <a href="@Url.Action("Index","Home")" class="nav-item nav-link nav-linkpack">Home</a>
                <a href="@Url.Action("Package","Home")" class="nav-item nav-link nav-linkpack">Packages</a>
                <a href="@Url.Action("Accomodation","Home")" class="nav-item nav-link nav-linkpack">Accomodation</a>
                <a href="@Url.Action("Activities","Home")" class="nav-item nav-link nav-linkpack">Activities</a>
                <a href="@Url.Action("Gallery","Home")" class="nav-item nav-link nav-linkpack">Pages</a>
                <a href="@Url.Action("Contact","Home")" class="nav-item nav-link">Contact</a>
            </div>
        </div>
    </nav>
</div>
<body>
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="row">
                    <div class="login100-pic js-tilt" data-tilt style="position:relative;top:-150px;right:10px;left:220px">
                        <img src="~/admin/assets/img/layouts/logo2_2cc3.png" alt="IMG">
                    </div>
                </div>
                <span class="login100-form-title" style="position:relative;top:-100px">
                    Register
                </span>
                <form></form>
                <form asp-action="RegisterPage" asp-controller="Login" method="post" id="manage-Login-form" enctype="multipart/form-data">
                    <div class="row" style="position:relative;top:-100px">
                        <div class="col-md-6">
                            <div class="wrap-input100 validate-input">
                                <input class="input100" type="text" asp-for="FullName" placeholder="Full Name" autocomplete="off">
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="fa fa-user" aria-hidden="true"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="wrap-input100 validate-input">
                                <input class="input100" type="text" asp-for="Username" placeholder="User Name" autocomplete="off">
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="fa fa-user" aria-hidden="true"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="wrap-input100 validate-input">
                                <input class="input100" type="text" asp-for="ContactNo" placeholder="Contact Number" autocomplete="off">
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="fa fa-phone" aria-hidden="true"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="wrap-input100 validate-input">
                                <input class="input100" type="text" asp-for="EmailId" placeholder="Email" autocomplete="off">
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="fa fa-envelope" aria-hidden="true"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="wrap-input100 validate-input">
                                <input class="input100" type="password" placeholder="Password">
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="fa fa-lock" aria-hidden="true"></i>
                                </span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="wrap-input100 validate-input">
                                <input class="input100" type="password" asp-for="UserPassword" placeholder="Confirm Password">
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="fa fa-lock" aria-hidden="true"></i>
                                </span>
                            </div>
                        </div>
                        <div class="container-login100-form-btn">
                            <button class="login100-form-btn">
                                Register
                            </button>
                        </div>
                        <div class="text-center p-t-136">
                            <a class="txt2" href="@Url.Action("Index","Login")">
                                Login
                                <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
                            </a>
                        </div>
                    </div>
                   
                </form>
            </div>
        </div>
    </div>
    <!--===============================================================================================-->
    <script src="~/login/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/login/vendor/bootstrap/js/popper.js"></script>
    <script src="~/login/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/login/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/login/vendor/tilt/tilt.jquery.min.js"></script>
    <script>
        $('.js-tilt').tilt({
            scale: 1.1
        })
        $(document).ready(function () {

        })
    </script>
    <!--===============================================================================================-->
    <script src="~/login/js/main.js"></script>
</body>
</html>
