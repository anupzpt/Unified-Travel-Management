@model HotelApp.Shared.Admin.Vechicle.VechicleCommon;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Vechicle Setup Information";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div style="display:flex;justify-content:space-between">
    <div>
        <h5 class="py-3 mb-4"><span class="text-muted fw-light text-primary">System /Vechicle Information/</span>List</h5>
    </div>
    <div style="display:flex;justify-content:flex-end">
        <div style="position:relative;right:10px"><button type="button" class="btn btn-default" onclick="javascript:history.go(-1);" style="background-color:gainsboro"><i class='bx bx-arrow-back'></i> Back</button></div>
    </div>
</div>
<hr />
<div class="card p-2">
    <section class="section-listing p-2">
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="nowrap" id="vechicleManagement" width="100%" style="border-top:5px solid white">
                        <thead style="background-color:#13357b;color:white">
                            <tr>
                                <th>SN</th>
                                <th>Vechicle Code</th>
                                <th>Name</th>
                                <th>Brand</th>
                                <th>Model</th>
                                <th>RegistrationNo</th>
                                <th>Status</th>
                                <th data-priority="1">Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </section>
</div>
@section scripts {
    <script src="~/scripts/Admin/Vechicle/vechicleSetup.js"></script>
    <partial name="_AlertPartial" />
    <script type="text/javascript">
        $(function () {
            var vechicleSetup = VechicleSetupManagement.vechicleSetup();
            vechicleSetup.renderIndex('@Model.VechicleType');
        });
    </script>
}